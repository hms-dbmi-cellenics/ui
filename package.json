{
  "name": "ui",
  "version": "0.0.0",
  "description": "Biomage App UI",
  "main": "server.js",
  "author": "Biomage Developers",
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "build": "next build",
    "prod": "NODE_ENV=production node server.js",
    "pretest": "eslint ./src",
    "test": "jest --silent --runInBand --bail",
    "coverage": "jest --coverage --silent",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "lint": "eslint ./src",
    "detect-secrets": "pip3 install detect-secrets && git diff --staged --name-only | xargs detect-secrets-hook --baseline .secrets.baseline",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "pre-commit": [
    "lint",
    "detect-secrets"
  ],
  "keywords": [
    "nextjs",
    "antd",
    "react",
    "ssr",
    "redux",
    "redux-thunk"
  ],
  "dependencies": {
